  "license": "MIT",
  "homepage": "https://github.com/tkuminecz/phantasy#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkuminecz/phantasy.git"
  },
  "bugs": {
    "url": "https://github.com/tkuminecz/phantasy/issues"
  },
  "main": "lib/main.js",
  "jsnext:main": "src/main.js",
  "files": [
    "lib/",
    "index.js"
  ],
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "build": "make clean && make",
    "flow": "flow",
    "test:tap": "BABEL_ENV=test babel-tape-runner $(\\find test | \\grep \\.js)",
    "test": "npm run test:tap | tap-diff",
    "test:coverage": "nyc --reporter=html --reporter=lcov npm test"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-root-import": "^4.1.3",
    "babel-tape-runner": "^2.0.1",
    "flow-bin": "0.36.0",
    "nyc": "^10.0.0",
    "tap-diff": "^0.1.1",
    "tape": "^4.6.2"
  },
  "dependencies": {
    "bluebird": "^3.4.6"
  }
}
